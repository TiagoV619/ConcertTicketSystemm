@model ConcertTicketSystem.DAL.Entity.Ticket

@{
    ViewData["Title"] = "Create";
}

<h1>Crear Boleta</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div>
                <partial name="../Shared/PartialViews/_TicketsEditCreate.cshtml" />
            </div>

            <div class="form-group">
                <input type="submit" value="Guardar boleta" class="btn btn-outline-primary" /> |
                <a asp-action="Index" class="btn btn-outline-success">Regresar al registro</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
