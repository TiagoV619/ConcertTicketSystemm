@model ConcertTicketSystem.DAL.Entity.Ticket

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar Boleta</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />

            <div>
                <partial name="../Shared/PartialViews/_TicketsEditCreate.cshtml" />
            </div>

            <div class="form-group">
                <input type="submit" value="Guardar cambios" class="btn btn-outline-primary" /> |
                <a asp-action="Index" class="btn btn-outline-success">Regresar al registro</a>
            </div>
        </form>
    </div>
</div>

<div>
   
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
